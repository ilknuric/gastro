<footer id="footer" class="footer background-black">
    <div class="footer-wrap wow fadeIn" data-wow-delay="0s">
        <div class="footer-body">
            <div class="container">
                <div class="row">
                    <div class="col-xl-5 col-md-6 col-12">
                        <div class="footer-infor">
                            <div class="footer-logo">
                                <a href="/">
                                    <img src="/logo.png" alt="gastro" width="150">
                                </a>
                            </div>
                            <% business.forEach((business, index) => { %>
                            <ul>
                                <li>
                                    <p><%- business.businessName %><br> <%- business.businessStreet %> <%- business.businessNr %>, <br> <%- business.businessPostcode %> <%- business.businessOrt %></p>
                                </li>
                                <li>
                                    <p>Email: <%- business.businessMail %></p>
                                </li>
                                <li>
                                    <p>Tel: <%- business.businessTel %></p>
                                </li>
                            </ul>
                            <% }); %> 
                        </div>
                    </div>
                    <div class="col-xl-4 col-md-6 col-12 footer-col-block">
                        <div class="footer-heading footer-heading-desktop">
                            <h6>Sitemap</h6>
                        </div>
                        <div class="footer-heading footer-heading-moblie">
                            <h6>Sitemap</h6>
                        </div>
                        <ul class="footer-menu-list tf-collapse-content">
                            <li>
                                <a href="/" class="footer-menu_item">Home</a>
                            </li>
                            <li>
                                <a href="/shop" class="footer-menu_item">Shop</a>
                            </li>
                            <li> 
                                <a href="/uber-uns" class="footer-menu_item">Über uns</a>
                            </li>
                            <li> 
                                <a href="/contact" class="footer-menu_item">Kontakt</a>
                            </li>
                            <li> 
                                <a href="/agb" class="footer-menu_item">AGB</a>
                            </li>
                            <li> 
                                <a href="/impressum" class="footer-menu_item">Impressum</a>
                            </li>
                        </ul>
                    </div>
                    <div class="col-xl-3 col-md-6 col-12 footer-col-block">
                        <div class="footer-heading footer-heading-desktop">
                            <h6>Schnelle Links</h6>
                        </div>
                        <div class="footer-heading footer-heading-moblie">
                            <h6>Schnelle Links</h6>
                        </div>
                        <ul class="footer-menu-list tf-collapse-content">
                            <li>
                                <a href="/login" class="footer-menu_item">Anmelden</a>
                            </li>
                            <li> 
                                <a href="/register" class="footer-menu_item">Registrieren</a>
                            </li>
                            <li> 
                                <a href="/dashboard" class="footer-menu_item">Dashboard</a>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
        <div class="footer-bottom">
            <div class="container">
                <div class="row">
                    <div class="col-12">
                        <div class="footer-bottom-wrap d-flex gap-20 flex-wrap justify-content-between align-items-center">
                            <div class="footer-menu_item">© 2024 Gastro Koçak Handel GmbH. Alle Rechte vorbehalten</div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</footer>