<body>
    <div class="main-wrapper">
        <%- include('./sidebar.ejs') %>
        <%- include('./header.ejs') %>
        <div class="page-wrapper">
            <div class="content">
                <div class="row">
                    <div class="col-lg-7 col-sm-12 m-auto">
                        <div class="content-page-header">
                            <h5>Bearbeiten <%= brand.brandName %> </h5>
                        </div>
                        <form action="../edit-brand/<%= brand._id %>" method="post">
                            <div class="row">
                                <div class="col-lg-12">
                                    <div class="form-group">
                                        <label>Markenname</label>
                                        <input type="text" class="form-control" value="<%= brand.brandName %>" name="brandName">
                                    </div>
                                </div>
                                <div class="col-lg-12">
                                    <div class="btn-path">                                    
                                        <a href="/admin/brands" class="btn btn-cancel me-3">Storniern</a>
                                        <button type="submit" class="btn btn-submit "> Einreichen</button>
                                    </div>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div> 
    </div>
